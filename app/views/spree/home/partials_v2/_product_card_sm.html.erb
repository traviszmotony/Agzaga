<div class="product-card-sm">
  <a href="<%= spree::product_url(product.slug) %>" data-turbolinks="false" class="clear-link">
    <div class="product-image-sm-group">
      <%= image_tag product.image_url.present? ? image_cdn_link(product.image_url) : 'noimage_sq.png' , loading: 'lazy', alt: product.name, class: 'product-image-sm' %>

      <%= product_label(product.label_text, product.label_color, product.label_display_text_color) %>
    </div>
  </a>
</div>

