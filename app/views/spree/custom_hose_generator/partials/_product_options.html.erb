<% @hose_fittings.each do |v| %>
  <div class="variant-data" data-id="<%= v.id %>" data-text="<%= v.options_text %>,<%= v.price%>" data-currency="<%= v.prices.last.currency %>"></div>
  <div class="variant-stock" data-text="<%= v.can_supply?%>"></div>
<% end %>

<% @custom_hose_variants.each do |v| %>
  <div class="custom_hose_data" data-id="<%= v.id %>" data-text="<%= v.options_text %>,<%= v.price%> "></div>
<% end %>

<div class="row card-container">
  <div class="col-md-2 col-lg-2 section-3">
    <div class="form-group">
      <label for="exampleFormControlFile1">LENGTH</label>
      <div class="line"></div>
      <div class="input-group mb-3">
        <%= f.number_field :length, class: "form-control form-control-sm custom_length", max: 300, min: 1, placeholder: "--" %>
        <div class="input-group-append">
          <span class="input-group-text">ft.</span>
        </div>
      </div>
    </div>

    <div class="color-group">
      <label class="color" for="exampleFormControlFile1">COLOR</label>
      <div class="line"></div>

      <div class="radio-feild">
        <% @custom_hose_variants.each do |variant| %>
          <% can_supply = variant.can_supply?%>
          <div class="form-check">
            <label class="form-check-label <%='disable' if !can_supply %>">
              <%= variant.option_value('Hose Color') %>
              <div class="d-none" data-text="<%= variant.option_value('Hose Color') %>"></div>
              <input type="radio" name="custom_hose[variant_id]" value="<%= variant.id %> " <%='disabled' if !can_supply %>>
              <span class="checkmark  <%='disable' if !can_supply %>"></span>
            </label>
          </div>
        <% end %>
      </div>
    </div>

    <div class="error-message">*Please select a color/length</div>
  </div>

  <div class="block1 col-sm-6 col-md-5 col-lg-5 col-xl-4 row ml-0 mr-0">
