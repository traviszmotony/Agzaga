      <label class="product-card" for="fitting_1_Aluminum">
        <div class="name">
          Anodized Aircraft Aluminum
        </div>
        <div class="image-container">
          <%= image_tag 'custom_hose/custom-hose_aluminum_female_zilla-green.jpg' %>
        </div>
      </label>
    </div>

    <div class="col-sm-12 col-md-6 col-lg-6 section-2">
