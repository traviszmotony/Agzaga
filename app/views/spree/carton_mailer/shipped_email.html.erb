<% email_root_url = "https://#{ENV['HOST']}" %>

<div>
  <a href="<%= email_root_url %>"
     style="text-decoration: none;
            display: block;
            margin: auto;">
    <img src="https://agazon-storage.nyc3.cdn.digitaloceanspaces.com/public_images/agzaga_logo.png" alt="Agzaga"
         style="width: 104px;
         display: block;
         margin: 26px auto;">
  </a>
</div>

<% if @order.user.present? %>
  <div style="background: #171717;
              padding: 16px;
              margin-bottom: 16px;">

    <p style="font-family: 'Google Sans' ,sans-serif;
              font-size: 28px;
              line-height: 28px;
              letter-spacing: 0.03em;
              text-transform: uppercase;
              color: #FFFFFF;
              margin: 0 0 6px 0;
              text-align: center;">
      Get 5% Off Your Next Order
    </p>

    <p style="font-family: 'Google Sans' ,sans-serif;
              font-style: normal;
              font-weight: bold;
              font-size: 9px;
              line-height: 12px;
              letter-spacing: 0.02em;
              margin: 0 auto 10px auto;
              color: #FFFFFF;
              text-align: center;">
     Our valued Members get 5% off their next order. Simply use promocode BACK4MORE while logged in at checkout!
    </p>

    <a href="<%= email_root_url + "/products/deals" %>"
        style=" display: block;
                cursor: pointer;
                text-align: center;
                font-family: 'Google Sans', sans-serif;
                background: #F1B71C;
                border-radius: 20px;
                width: 262px;
                padding: 8px 0;
                font-style: normal;
                font-weight: 600;
                font-size: 12px;
                line-height: 18px;
                letter-spacing: 0.5px;
                color: #FFFFFF;
                margin: 0 auto 0 auto;
                text-decoration: none;">
      Shop Deals Now
    </a>

  </div>
<% else %>
  <div style="background: #171717;
              padding: 16px;
              margin-bottom: 16px;">

    <p style="font-family: 'Google Sans' ,sans-serif;
              font-size: 28px;
              line-height: 28px;
              letter-spacing: 0.03em;
              text-transform: uppercase;
              color: #FFFFFF;
              margin: 0 0 6px 0;
              text-align: center;">
      Get 10% off your next order
    </p>

    <p style="font-family: 'Google Sans' ,sans-serif;
              font-style: normal;
              font-weight: bold;
              font-size: 9px;
              line-height: 12px;
              letter-spacing: 0.02em;
              margin: 0 auto 10px auto;
              color: #FFFFFF;
              text-align: center;">
      Create a Member Account and use promo code WELCOME10 while logged in at checkout!
    </p>

    <a href="<%= email_root_url + "?login=true" %>"
        style=" display: block;
                cursor: pointer;
                text-align: center;
                font-family: 'Google Sans', sans-serif;
                background: #F1B71C;
                border-radius: 20px;
                width: 262px;
                padding: 8px 0;
                font-style: normal;
                font-weight: 600;
                font-size: 12px;
                line-height: 18px;
                letter-spacing: 0.5px;
                color: #FFFFFF;
                margin: 0 auto 0 auto;
                text-decoration: none;">
      Join Now
    </a>

  </div>
<% end %>


<div style="margin: 16px auto 4px auto;
            width: 48px;">
  <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/Truck_black.png" alt="Agzaga"
         style="width: 48px;">
</div>

<p style="font-family: 'Google Sans', sans-serif;
            font-size: 36px;
            line-height: 38px;
            text-align: center;
            letter-spacing: 0.03em;
            color: #000000;
            text-transform: uppercase;
            padding: 0 48px;
            font-weight: 500;
            margin: 0 0 8px 0;">
  Your order’s on its way.
</p>

<p style="font-family: 'Google Sans', sans-serif;
            font-style: normal;
            font-weight: normal;
            font-size: 12px;
            line-height: 17px;
            text-align: center;
            letter-spacing: 0.03em;
            color: #000000;
            text-transform: uppercase;
            padding: 0 48px;
            margin: 0 0 16px 0;">
  To track your package, click the link below.
</p>

<a href="<%= @order.shipments.last.tracking_lookup %>"
  style="text-decoration: none;
         width: fit-content;
         display: block;
         margin: 0 auto;">
  <div style="font-family: 'Google Sans', sans-serif;
          background: #51C571;
          border-radius: 8px;
          font-style: normal;
          font-weight: bold;
          font-size: 14px;
          line-height: 20px;
          text-align: center;
          margin: 0 auto 16px auto;
          max-width: 200px;
          width: 100%;
          padding: 10px 12px;
          letter-spacing: 0.16em;
          color: #FFFFFF;
          text-transform: uppercase;">
    Track My Order
  </div>
</a>

<div style="width: 100%;
            text-align: center;
            font-family: 'Google Sans', sans-serif;
            font-style: normal;
            margin-bottom: 24px;">
  <div style="font-weight: 400;
              font-size: 12px;
              line-height: 17px;
              letter-spacing: 0.03em;
              color: #000000;
              margin-bottom: 4px;">
