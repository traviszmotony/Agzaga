  </div>

  <div style="font-weight: 700;
              font-size: 14px;
              line-height: 20px;
              letter-spacing: 0.16em;
              color: #18181B;">
    <%= @order.shipments.last.tracking %>
  </div>
</div>

<div class="order-group"
      style="max-width: 536px;
              background: #FAFAFA;
              box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.04), 0px 2px 6px rgba(0, 0, 0, 0.04), 0px 0px 1px rgba(0, 0, 0, 0.04);
              border-radius: 16px;
              margin: 0 auto 16px;
              padding: 30px 0 18px 0;">

  <p style="font-family: Poppins-bold, sans-serif;
            font-style: normal;
            font-weight: bold;
            font-size: 18px;
            line-height: 23px;
            cursor: default;
            color: #171717;
            letter-spacing: 0.02em;
            margin: 0 12px 16px 12px;">
    What’s In Your Order
  </p>

  <% @order.line_items.each do |item| %>
    <table style="margin: 0 12px 16px 12px; width: 96%">
      <tr style="margin-bottom: 3px;">
        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: bold;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;max-width: 360px;">
          <%= item.variant.product.name %>
        </td>

        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: bold;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
          <%= item.display_amount %>
        </td>
      </tr>

      <tr>
        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 10px;line-height: 14px;letter-spacing: 0.03em;color: #000000;cursor: default;">
          <%= item.variant.options_text %>
        </td>

        <td style="text-align: right;">
          <p style="font-family: sans-serif;font-weight: normal;font-size: 8px;line-height: 11px;text-align: right;letter-spacing: 0.03em;color: #000000;cursor: default;font-style: italic; margin: 0;">
            SKU: <span><%= item.variant.sku %></span>
          </p>

          <div style="font-family: sans-serif;font-weight: normal;font-size: 8px;line-height: 11px;text-align: right;letter-spacing: 0.03em;color: #000000;cursor: default;font-style: italic; margin: 0;">
            Qty: <span><%= item.quantity %></span>
          </div>
        </td>
      </tr>
    </table>
  <% end %>
</div>

