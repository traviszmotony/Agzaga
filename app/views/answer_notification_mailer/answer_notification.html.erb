<% email_root_url = "https://#{ENV['HOST']}" %>

<div style="  width: 100%;
              display: flex;
              align-items: center;
              justify-content: center;
              color: #FFF;
              margin-bottom: 8px;">
  <a href="<%= email_root_url %>"
     style="text-decoration: none;
            display: block;
            margin: auto;">
    <img src="https://agazon-storage.nyc3.cdn.digitaloceanspaces.com/public_images/agzaga_logo.png" alt="Agzaga" style="width: 104px; display: block; margin: auto; ">
  </a>
</div>

<div
  style=" text-align: center;
          font-family: Poppins, sans-serif;
          margin-bottom: 16px;">

  <a href="<%= spree::account_url %>"
     style="font-size: 10px;
            line-height: 16px;
            letter-spacing: 0.04em;
            color: #171717;
            text-decoration: none;
            display: inline-block;
            cursor: pointer;
            margin: 0 16px;">
    Account Info
  </a>

  <a href="<%= spree::product_categories_url %>"
     style="font-size: 10px;
            letter-spacing: 0.04em;
            color: #171717;
            text-decoration: none;
            display: inline-block;
            cursor: pointer;
            margin: 0 16px;">
    View All Products
  </a>
</div>

<div class="thank-you-heading" style="font-family: Draught, sans-serif;font-style: normal;font-weight: normal;font-size: 45px;line-height: 46px; align-items: center; text-align: center; letter-spacing: 0.03em; color: #000000; margin-bottom: 16px; cursor: default;padding: 0 45px;">
  Thank you for your question!
</div>

<p class="sub-head-text" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;align-items: center;text-align: center;letter-spacing: 0.03em;color: #000000;max-width: 384px;margin: 0 auto 24px;padding: 0 34px;cursor: default;">
  We appreciate your interest in one of our great products. Our team has reviewed your question and provided the answer below.
</p>

<div style="text-align: center;">
  <img style="border: 1px solid #E4E4E7;border-radius: 8px;margin-bottom: 12px;" src="<%= image_cdn_link(@product_question.product.gallery.images.first.url(:sm_200)) %>" alt="Product Image">

  <div style="font-family: sans-serif;"><%= @product_question.product.name %></div>
</div>

<div class="for-desktop order-status-btn" style="text-align: center; margin: 23px;">
  <a href="<%= spree::product_url(@product_question.product.slug) %>"
      style=" padding: 11px;
              margin: 0 0 15px 0;
              font-size: 12px;
              line-height: 18px;
              font-weight: 600;
              background: linear-gradient(145.87deg, #F1B71C -11.86%, #EB6E19 95.89%);
              box-shadow: 0px 2px 6px rgb(240 170 28 / 45%);
              border-radius: 100px;
              color: #FFFFFF;
              border: none;
              outline: none;
              cursor: pointer;
              text-decoration: none;
              font-family: Draught, sans-serif;">
    View This Product
  </a>
</div>

<div class="question" style="
                          width: 100%;
                          max-width: 536px;
                          margin: 0 auto;
                          margin-bottom: 20px;">
  <div style="font-family: Poppins, sans-serif;font-style: normal;font-weight: 600;font-size: 14px;line-height: 20px;letter-spacing: 0.04em;color: var(--palette-neutral-900);margin-bottom: 2px"><span style="font-weight: 700;font-weight: bold;">Q: </span> <%= @product_question.question %></div>
  <div style="font-family: Poppins, sans-serif;font-style: normal;font-weight: 400;font-size: 14px;line-height: 20px;letter-spacing: 0.04em;color: var(--palette-neutral-600);margin-bottom: 2px;"><span style="font-weight: 700;font-weight: bold;">A: </span> <%= @product_question.answer %></div>

</div>

<table class="any-question-group" style="width:100%; max-width: 536px; height: 85px; background: #FAFAFA; box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.04), 0px 2px 6px rgba(0, 0, 0, 0.04), 0px 0px 1px rgba(0, 0, 0, 0.04); border-radius: 16px; margin: 0 auto 24px auto;">
  <tr>
    <td>
      <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/email_icon.png" style="width: 40px; height: 32px; margin: auto 16px auto 12px;">
    </td>

    <td>
      <div class="poppins-bold-text-12" style="font-family: Poppins, sans-serif; font-style: normal; font-weight: bold; font-size: 12px;line-height: 17px; letter-spacing: 0.03em; color: #000000; cursor: default;">More Questions?</div>
      <div class="poppins-text-10" style="font-family: Poppins, sans-serif; font-style: normal; font-weight: normal; font-size: 10px;line-height: 14px; letter-spacing: 0.03em; color: #000000; cursor: default; margin-right: 8px;">
        Email us anytime at
        <a href="mailto:support@agzaga.com" target="_blank" style=" color: #7b8c48; text-decoration:underline;">support@agzaga.com</a>
        or call us at
        <a href="tel:+1-888-249-7412" target="_blank" style=" color: #7b8c48; text-decoration:underline;">1-888-249-7412</a>
      </div>
    </td>
  </tr>
</table>

<div style="max-width: 536px; height: 85px; background: #FAFAFA; border-radius: 16px; margin-bottom: 37px; margin: 0 auto 30px auto; display: block; text-align: center;">
  <div class="poppins-bold-text-12" style="font-family: sans-serif;font-style: normal;font-weight: bold;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; padding: 10px 0 13px 0;">
    Following Us?
  </div>

  <div>
    <a target="_blank" href="https://www.facebook.com/agzagasocial" style="text-decoration: none; display: inline-block; width: 32px; height: 32px; margin-right: 12px;">
      <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/facebook.png" style="width: 32px">
    </a>

    <a target="_blank" href="https://www.instagram.com/agzagasocial" style="text-decoration: none; display: inline-block; width: 32px; height: 32px; margin-right: 12px;">
      <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/instagram.png" style="width: 32px">
    </a>

    <a target="_blank" href="https://www.twitter.com/agzagasocial" style="text-decoration: none; display: inline-block; width: 32px; height: 32px; margin-right: 12px;">
      <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/Twitter.png" style="width: 32px">
    </a>
    <a target="_blank" href="https://www.youtube.com/channel/UCbakHBOpI1LJAMsqTvmE13w" style="text-decoration: none; display: inline-block; width: 32px; height: 32px;">
      <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/youtube.png" style="width: 32px">
    </a>
  </div>
</div>

<div class="dark-footer" style="width: 100%; height: 40px; background: #171717; display: flex;align-items: center; justify-content: center; color: #FFF;">
  <img src="https://agazon-storage.nyc3.digitaloceanspaces.com/public_images/AGZ_logo_mark.png" alt="AGzaga" class="logo" style="width: 15px; height: 22px; margin: auto; display: block;">
</div>

